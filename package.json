{
	"name": "@microlabs/otel-cf-workers",
	"version": "1.0.0-rc.40",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/esm/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/esm/index.d.ts",
				"default": "./dist/esm/index.js"
			},
			"require": {
				"types": "./dist/cjs/index.d.ts",
				"default": "./dist/cjs/index.js"
			}
		}
	},
	"scripts": {
		"clean": "rimraf ./dist",
		"format": "prettier --ignore-unknown --write .",
		"build-esm": "tsc",
		"build-cjs": "tsc -p tsconfig-cjs.json",
		"fix-dist": "echo '{\"type\": \"module\"}' > dist/esm/package.json && echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json",
		"build": "run-s -l clean build-esm build-cjs fix-dist",
		"release": "pnpm run build && changeset publish",
		"preversion": "run-s -l check:* build",
		"prepublishOnly": "run-s -l check:* build",
		"check": "run-s -l check:*",
		"check:format": "prettier . --check --ignore-unknown",
		"check:type-esm": "tsc --noEmit",
		"check:type-cjs": "tsc --noEmit -p tsconfig-cjs.json",
		"watch": "tsc --watch",
		"lint": "eslint",
		"prepare": "husky"
	},
	"homepage": "https://github.com/evanderkoogh/otel-cf-workers#readme",
	"dependencies": {
		"@opentelemetry/api": "~1.8.0",
		"@opentelemetry/core": "^1.25.1",
		"@opentelemetry/exporter-trace-otlp-http": "^0.51.1",
		"@opentelemetry/otlp-exporter-base": "^0.51.1",
		"@opentelemetry/otlp-transformer": "0.51.1",
		"@opentelemetry/resources": "^1.25.1",
		"@opentelemetry/sdk-trace-base": "^1.25.1",
		"@opentelemetry/semantic-conventions": "^1.25.1"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.7",
		"@cloudflare/workers-types": "^4.20240620.0",
		"@eslint/js": "^9.6.0",
		"@stylistic/eslint-plugin": "^2.3.0",
		"globals": "^15.8.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.3.2",
		"rimraf": "^6.0.1",
		"typescript": "^5.5.3",
		"typescript-eslint": "^7.16.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"license": "BSD-3-Clause",
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"packageManager": "pnpm@8.15.6+sha512.77b89e9be77a2b06ad8f403a19cae5e22976f61023f98ad323d5c30194958ebc02ee0a6ae5d13ee454f6134e4e8caf29a05f0b1a0e1d2b17bca6b6a1f1159f86"
}
