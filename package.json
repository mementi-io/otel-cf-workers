{
  "name": "@mementi-io/otel-cf-workers",
  "version": "1.1.1",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mementi-io/otel-cf-workers.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.5.0",
  "license": "BSD-3-Clause",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build-esm": "tsc",
    "build-cjs": "tsc -p tsconfig-cjs.json",
    "fix-dist": "echo '{\"type\": \"module\"}' > dist/esm/package.json && echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json",
    "prebuild": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version).replaceAll('\\\"', \\\"'\\\") + ';'\" > src/version.ts",
    "build": "run-s -l clean build-esm build-cjs fix-dist",
    "release": "pnpm run build && changeset publish",
    "preversion": "run-s -l check:* build",
    "prepublishOnly": "run-s -l check:* build",
    "check": "run-s -l check:*",
    "check:type-esm": "tsc --noEmit",
    "check:type-cjs": "tsc --noEmit -p tsconfig-cjs.json",
    "watch": "tsc --watch",
    "lint": "eslint --cache"
  },
  "dependencies": {
    "@opentelemetry/api": "~1.8.0",
    "@opentelemetry/core": "^1.25.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.51.1",
    "@opentelemetry/otlp-exporter-base": "^0.51.1",
    "@opentelemetry/otlp-transformer": "0.51.1",
    "@opentelemetry/resources": "^1.25.1",
    "@opentelemetry/sdk-trace-base": "^1.25.1",
    "@opentelemetry/semantic-conventions": "^1.25.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@cloudflare/workers-types": "^4.20240701.0",
    "@eslint/js": "^9.9.1",
    "@stylistic/eslint-plugin": "^2.3.0",
    "globals": "^15.8.0",
    "lint-staged": "^15.2.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0"
  }
}
